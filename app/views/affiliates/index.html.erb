<h1>Listing affiliates</h1>

<table>
  <tr>
    <th>Znumber</th>
    <th>Prefix</th>
    <th>Firstname</th>
    <th>Middlename</th>
    <th>Lastname</th>
    <th>Suffix</th>
    <th>License</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @affiliates.each do |affiliate| %>
  <tr>
    <td><%= affiliate.znumber %></td>
    <td><%= affiliate.prefix %></td>
    <td><%= affiliate.firstname %></td>
    <td><%= affiliate.middlename %></td>
    <td><%= affiliate.lastname %></td>
    <td><%= affiliate.suffix %></td>
    <td><%= affiliate.license %></td>
    <td><%= link_to 'Show', affiliate %></td>
    <td><%= link_to 'Edit', edit_affiliate_path(affiliate) %></td>
    <td><%= link_to 'Destroy', affiliate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Affiliate', new_affiliate_path %>
