<%= form_for(@annualevaluation) do |f| %>
  <% if @annualevaluation.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@annualevaluation.errors.count, "error") %> prohibited this annualevaluation from being saved:</h2>

      <ul>
      <% @annualevaluation.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <!--
  <div class="form-group">
    <%= f.label :duedate %><br />
    <%= f.text_field :duedate %>
  </div>
  -->


  <div class="form-group">
    <div class="input-group">
      <%= f.label :duedate, "Due Date", :class=>'control-label' %><br />
      <%= f.text_field :duedate  , :class=>'form-control' , 'data-mask' => "99/99/9999" %>
      <span class="help-block">99/99/9999</span>
    </div>
  </div>

  <div class="form-group">
     <div class="input-group">
      <%= f.label :completeddate , "Completed Date" , :class=>'control-label' %><br />
      <%= f.text_field :completeddate  , :class=>'form-control' , 'data-mask' => "99/99/9999" %>
      <span class="help-block">99/99/9999</span>
     </div>
  </div>
  <div class="form-group">
    <%= f.label :evaluator_id, "Evaluator" %><br />
    <%= f.number_field :evaluator_id , :class=>'form-control'%>
  </div>
  <div class="form-group">
    <%= f.label :status_id, "Status" %><br />
    <%= f.number_field :status_id , :class=>'form-control' %>
  </div>
  <div class="actions">
    <%= f.submit 'Update Evaluation' %>
  </div>
<% end %>
