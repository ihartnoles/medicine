<h1>Listing annualevaluations</h1>

<table>
  <tr>
    <th>Duedate</th>
    <th>Completeddate</th>
    <th>Evaluator</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @annualevaluations.each do |annualevaluation| %>
  <tr>
    <td><%= annualevaluation.duedate %></td>
    <td><%= annualevaluation.completeddate %></td>
    <td><%= annualevaluation.evaluator_id %></td>
    <td><%= annualevaluation.status_id %></td>
    <td><%= link_to 'Show', annualevaluation %></td>
    <td><%= link_to 'Edit', edit_annualevaluation_path(annualevaluation) %></td>
    <td><%= link_to 'Destroy', annualevaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Annualevaluation', new_annualevaluation_path %>
