<h1>Listing certifiedspecialities</h1>

<table>
  <tr>
    <th>Affiliate</th>
    <th>Speciality</th>
    <th>Year</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @certifiedspecialities.each do |certifiedspeciality| %>
  <tr>
    <td><%= certifiedspeciality.affiliate_id %></td>
    <td><%= certifiedspeciality.speciality_id %></td>
    <td><%= certifiedspeciality.year %></td>
    <td><%= link_to 'Show', certifiedspeciality %></td>
    <td><%= link_to 'Edit', edit_certifiedspeciality_path(certifiedspeciality) %></td>
    <td><%= link_to 'Destroy', certifiedspeciality, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

