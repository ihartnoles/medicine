<h1>Listing degrees</h1>

<table>
  <tr>
    <th>Degreename</th>
    <th>Year</th>
    <th>Institution</th>
    <th>Affiliate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @degrees.each do |degree| %>
  <tr>
    <td><%= degree.degreename %></td>
    <td><%= degree.year %></td>
    <td><%= degree.institution_id %></td>
    <td><%= degree.affiliate_id %></td>
    <td><%= link_to 'Show', degree %></td>
    <td><%= link_to 'Edit', edit_degree_path(degree) %></td>
    <td><%= link_to 'Destroy', degree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Degree', new_degree_path %>
