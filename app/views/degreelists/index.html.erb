<h1>Listing degreelists</h1>

<table>
  <tr>
    <th>Degreename</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @degreelists.each do |degreelist| %>
  <tr>
    <td><%= degreelist.degreename %></td>
    <td><%= link_to 'Show', degreelist %></td>
    <td><%= link_to 'Edit', edit_degreelist_path(degreelist) %></td>
    <td><%= link_to 'Destroy', degreelist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

